{% extends 'Map3CoreBundle::layout.html.twig' %}

{% block title %}{{ parent() }} - Reference delete{% endblock title %}

{% block headline %}{{ baseline_breadcrumb('Delete reference') }}{% endblock headline %}

{% block content %}
<div class="row-fluid">
    {% include "Map3BaselineBundle:Baseline:tabs.html.twig" with {'activeTab' : 'references'} %}
</div>
<br />
<div class="row">
    <div class="col-md-6 col-md-offset-3">

        {% include "Map3CoreBundle::formDel.html.twig" %}

    </div>
</div>
<div class="row-fluid">
    <a class="btn btn-default btn-sm" href="{{ path('bln-ref_index') }}"
       title="Return to list">
        {{ icon('chevron-left') }} Return to list
    </a>
</div>

{% endblock content %}
